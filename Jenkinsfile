pipeline {
  agent any
  stages {
    stage('Buzz Buzz') {
      steps {
        echo 'Hello World'
      }
    }

    stage('Bees Bees') {
      steps {
        echo 'shammi timmi'
        echo 'Buzzing bees'
        echo 'Bees Buzzing Again'
      }
    }
    
    stage('bum bum') {
      steps {
        echo 'shammi timmi'
        echo 'Buzzing bees'
        echo 'Bees Buzzing Again'
      }
    }
  }
}
